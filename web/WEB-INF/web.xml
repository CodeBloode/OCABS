<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <!--login servlet-->
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>ViewAllCounsellors</servlet-name>
        <servlet-class>dean.ViewAllCounsellors</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <!--end login servlet-->
    <!--signup servlet-->
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup.signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <!--end signupservlet-->
    <!--dean chagepass-->
    <servlet>
        <servlet-name>DeanChangePassword</servlet-name>
        <servlet-class>dean.DeanChanegePasswordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeanChangePassword</servlet-name>
        <url-pattern>/DeanChangePassword</url-pattern>
    </servlet-mapping>
    <!--end dean change servlet-->
    <!--login error servlet-->
    <servlet>
        <servlet-name>loginError</servlet-name>
        <servlet-class>supportservlet.loginError</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginError</servlet-name>
        <url-pattern>/loginError</url-pattern>
    </servlet-mapping>
    <!--end logi n error servlet-->
    <!--counsellor servlet-->
    <servlet>
        <servlet-name>CounsellorSignup</servlet-name>
        <servlet-class>counsellor.CounsellorSignup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CounsellorSignup</servlet-name>
        <url-pattern>/CounsellorSignup</url-pattern>
    </servlet-mapping>
    <!--end counsellor servlet-->
    <servlet-mapping>
        <servlet-name>ViewAllCounsellors</servlet-name>
        <url-pattern>/ViewAllCounsellors</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
