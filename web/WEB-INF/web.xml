<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login.login</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup.signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeanChangePassword</servlet-name>
        <servlet-class>dean.DeanChanegePasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CounsellorSetSechedule</servlet-name>
        <servlet-class>counsellor.CounsellorSetSechedule</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>counsellor.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetSechServlet</servlet-name>
        <servlet-class>counsellor.SetSechServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletDAO</servlet-name>
        <servlet-class>counsellor.ServletDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchView</servlet-name>
        <servlet-class>counsellor.searchView</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeanChangePassword</servlet-name>
        <url-pattern>/DeanChangePassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CounsellorSetSechedule</servlet-name>
        <url-pattern>/CounsellorSetSechedule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetSechServlet</servlet-name>
        <url-pattern>/SetSechServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletDAO</servlet-name>
        <url-pattern>/ServletDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchView</servlet-name>
        <url-pattern>/searchView</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
